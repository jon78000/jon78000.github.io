// Function to animate the title
const animateTitle = () => {
  const titleText = [":/", ":3", ":c"];
  let index = 0;

  setInterval(() => {
    document.title = titleText[index];
    index = (index + 1) % titleText.length; // Loop back to the start
  }, 1000); // Change title every second
};

// Call the function when the page loads
window.onload = animateTitle;
